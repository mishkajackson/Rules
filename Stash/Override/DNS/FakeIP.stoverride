name: FakeIP
category: DNS
icon: https://raw.githubusercontent.com/Koolson/Qure/refs/heads/master/IconSet/Color/Area.png
desc: DNS with Fake IP enabled
dns:
  enable: true
  enhanced-mode: fake-ip
  listen: 0.0.0.0:53
  fake-ip-range: 198.18.0.1/16
  fake-ip-filter:
    - ".*\\.lan$"
    - "localhost"
  
